{
    "store.storelocator": {
      "blocks": [
        "flex-layout.row#title", 
        "flex-layout.row#container"]
    },
    "flex-layout.row#title": {
      "children": ["flex-layout.col#title"],
      "props": {
        "blockClass": "home-block"
      }
    },
    "flex-layout.row#container": {
      "children": ["store-list"],
      "props": {
        "blockClass": ["home-block", "CardStoresContainer"]
      }
    },
    "flex-layout.col#title": {
      "children": ["rich-text#title"],
      "props": {
        "blockClass": "title",
        "preventVerticalStretch": true
      }
    },
    "rich-text#title": {
      "props": {
        "text": "Tiendas KAO"
      }
    },
    "store.storedetail": {
      "blocks": ["flex-layout.row#titleStore", "store-group"]
    },
    "store-group": {
      "children": ["flex-layout.row#containerStore"],
      "props": {
        "title": "{storeName} Store"
      }
    },
    "flex-layout.row#titleStore": {
      "children": ["flex-layout.col#titleStore"]
    },
    "flex-layout.row#containerStore": {
      "children": ["flex-layout.col#detail", "flex-layout.col#store"]
    },
    "flex-layout.col#titleStore": {
      "children": ["rich-text#titleStore"],
      "props": {
        "blockClass": "title",
        "preventVerticalStretch": true
      }
    },
    "rich-text#titleStore": {
      "props": {
        "text": "## Detalles de la tienda"
      }
    },
    "flex-layout.col#detail": {
      "children": [
        "store-back-link",
        "store-map",
        "store-address",
        "store-hours",
        "store-instructions"
      ],
      "props": {
        "width": "30%",
        "preventVerticalStretch": true
      }
    },
    "flex-layout.col#store": {
      "children": ["store-name"],
      "props": {
        "width": "70%"
      }
    },
    "store-hours": {
      "props": {
        "label": "Horario de Atenci√≥n:",
        "format": "12h",
        "businessHours": [
          {
            "dayOfWeek": "Domingo",
            "openingTime": "11:00am",
            "closingTime": "5:00pm"
          },
          {
            "dayOfWeek": "Lunes",
            "openingTime": "11:00am",
            "closingTime": "6:00pm"
          },
          {
            "dayOfWeek": "Martes",
            "openingTime": "11:00am",
            "closingTime": "6:00pm"
          },
          {
            "dayOfWeek": "Miercoles",
            "openingTime": "11:00am",
            "closingTime": "6:00pm"
          },
          {
            "dayOfWeek": "Jueves",
            "openingTime": "11:00am",
            "closingTime": "6:00pm"
          },
          {
            "dayOfWeek": "Viernes",
            "openingTime": "11:00am",
            "closingTime": "6:00pm"
          },
          {
            "dayOfWeek": "Sabado",
            "openingTime": "11:00am",
            "closingTime": "5:00pm"
          }
        ]
      }
    },
    "store-back-link": {
      "props": {
        "label": "Regresar al listado de tiendas"
      }
    },
    "store-instructions": {
      "props": {
        "label": ""
      }
    }
  }
  