{
  "store.home": {
    "blocks": [
      "list-context.image-list#home",
      "flex-layout.row#QuickAccess",
      "rich-text#nuevos-productos",
      "flex-layout.row#NuevosProductos",
      "modal-trigger#promo-modal",
      "__fold__",
      "rich-text#productos-destacados",
