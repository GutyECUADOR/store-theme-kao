        }
      ]
    }
  },
  "slider-layout#slides-images": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 6000,
        "stopOnHover": true
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel"
    }
  },

  "flex-layout.row#QuickAccess": {
    "children": ["flex-layout.row#QuickAccessCards"],
    "props": {
      "blockClass": "home-block"
